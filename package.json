{
  "name": "@rsmap/vue3ol",
  "version": "0.1.1",
  "description": "使用vue3风格封装后的 Openlayer 地图组件库。",
  "repository": {
    "type": "git",
    "url": "https://github.com/leolovesmile/vue3ol"
  },
  "homepage": "https://github.com/leolovesmile/vue3ol",
  "bugs": {
    "url": "https://github.com/leolovesmile/vue3ol/issues"
  },
  "keywords": [
    "前端",
    "web",
    "vue3",
    "openlayers",
    "map",
    "gis"
  ],
  "author": "gw rsmap",
  "license": "MIT",
  "private": false,
  "main": "dist/vue3ol.common.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name vue3ol src/index.js",
    "build-report": "vue-cli-service build --report",
    "lint": "vue-cli-service lint",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "docs:dev": "vitepress dev docs"
  },
  "files": [
    "dist/*"
  ],
  "dependencies": {
    "core-js": "^3.6.5",
    "file-saver": "^2.0.5",
    "jspdf": "^2.3.1",
    "ol": "^6.6.1",
    "ol-contextmenu": "^4.1.0",
    "ol-ext": "^3.2.4",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.11.1",
    "vitepress": "^0.15.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
